        <text macro="contributors-biblio"/>
        <text macro="title-note"/>
        <text macro="description-note"/>
         <text macro="volume1"/>
      <text macro="container-title1"/>
                <text macro="editor-translator"/>
                <text macro="volume1"/>
                <group delimiter=" ">
                  <text macro="issued-place"/>
                                  </group>>
        <text macro="secondary-contributors-note"/>
