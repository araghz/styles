  <!--MACROS-->
<macro name="secondary-contributors-note">
      <choose>
         <if type="chapter paper-conference" match="none">
            <text macro="editor-translator"/>
         </if>
      </choose>
   </macro>
   <macro name="container-contributors-note">
      <choose>
         <if type="chapter paper-conference" match="any">
            <text macro="editor-translator"/>
         </if>
      </choose>
   </macro>
   <macro name="editor-note">
      <names variable="editor">
         <name and="text" sort-separator=", " initialize-with="." delimiter=", "/>
         <label form="short" prefix=", " suffix="." strip-periods="true"/>
      </names>
   </macro>
   <macro name="translator-note">
      <names variable="translator">
         <name and="text" sort-separator=", " initialize-with="." delimiter=", "/>
         <label form="verb-short" prefix=", " suffix="." strip-periods="true" />
      </names>
   </macro>
   <macro name="recipient-note">
      <names variable="recipient" delimiter=", ">
         <label form="verb" prefix=" " text-case="lowercase" suffix=" "/>
         <name and="text" delimiter=", "/>
      </names>
   </macro>
   <macro name="contributors-note">
      <names variable="author">
         <name and="text" sort-separator=", " initialize-with="." delimiter=", "/>
         <substitute>
            <text macro="editor-note"/>
            <text macro="translator-note"/>
         </substitute>
      </names>
      <text macro="recipient-note"/>
   </macro>
   <macro name="recipient-short">
      <names variable="recipient">
         <label form="verb" prefix=" " text-case="lowercase" suffix=" "/>
         <name form="short" and="text" delimiter=", " />
      </names>
   </macro>
    <macro name="contributors-short">
      <names variable="author">
         <name and="text" delimiter=", " initialize-with="."/>
         <substitute>
            <names variable="editor"/>
            <names variable="translator"/>
         </substitute>
      </names>
      <text macro="recipient-short"/>
   </macro>
   <macro name="contributors-biblio">
      <names variable="author">
         <name and="text" name-as-sort-order="all" sort-separator=" " initialize-with="." delimiter=", " delimiter-precedes-last="always"/>
         <substitute>
            <names variable="editor"/>
            <names variable="translator"/>
         </substitute>
      </names>
      <text macro="recipient-short"/>
   </macro>
   <macro name="interviewer-note">
      <names variable="interviewer" delimiter=", ">
         <label form="verb" prefix=" " text-case="lowercase" suffix=" "/>
         <name and="text" delimiter=", "/>
      </names>
   </macro>
   <macro name="title-note">
      <choose>
         <if variable="title" match="none">
            <text variable="genre"/>
         </if>
         <else>
            <text variable="title" font-style="italic"/>
         </else>
      </choose>
   </macro>
  
  
  <!--Contributors-->
  <macro name="authors1">
    <names variable="author">
      <name sort-separator=", " initialize-with="." delimiter=", "/>
      <substitute>
        <text macro="editors1"/>
        <text macro="translators1"/>
      </substitute>
    </names>
  </macro>
  <macro name="editors1">
    <names variable="editor">
      <name sort-separator=", " initialize-with="." delimiter=", "/>
      <!--label form="short" prefix=", " /-->
    </names>
  </macro>
  <macro name="translators1">
    <names variable="translator">
      <name sort-separator=", " initialize-with="." delimiter=", "/>
      <label form="verb-short" prefix=", "/>
    </names>
  </macro>
  <macro name="editor-translator">
    <group delimiter=", ">
      <choose>
        <if variable="author">
          <names variable="editor" delimiter=", ">
            <label form="verb-short" text-case="lowercase" suffix=" "/>
            <name delimiter=", " initialize-with="."/>
          </names>
          <choose>
            <if variable="container-author">
              <group>
                <names variable="container-author">
                  <name delimiter=", "/>
                </names>
              </group>
            </if>
          </choose>
        </if>
      </choose>
      <choose>
        <if variable="author editor" match="any">
          <names variable="translator" delimiter=", ">
            <label form="verb-short" text-case="lowercase" suffix=" "/>
            <name delimiter=", " initialize-with="."/>
          </names>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="contributors-short">
    <names variable="author">
      <name and="text" delimiter=", " initialize-with="."/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <!--Titles-->
  <macro name="title1">
    <choose>
      <if variable="title" match="none">
        <text variable="genre"/>
      </if>
      <else>
        <text variable="title" font-style="italic"  prefix="" suffix=""/>
      </else>
      <else-if type="article-newspaper article-magazine" match="any">
        <text variable="title" font-style="normal" prefix="&#x201E;" suffix="&#x201D;"/>
        </else-if>
    </choose>
  </macro>
  <macro name="title2">
    <choose>
      <if variable="title" match="none">
        <text variable="genre"/>
      </if>
      <else>
        <text variable="title" font-style="italic" prefix="" suffix=""/>
      </else>
        <else-if type="article-newspaper article-magazine" match="any">
        <text variable="title" font-style="normal" prefix="&#x201E;" suffix="&#x201D;"/>
        </else-if>
    </choose>
  </macro>
    <macro name="title3">
    <choose>
      <if variable="title" match="none">
        <text variable="genre"/>
      </if>

        <else-if type="article-magazine" match="any">
        <text variable="title" font-style="normal" prefix="&#x201E;" suffix="&#x201D;"/>
        </else-if>
    </choose>
  </macro>
    <macro name="title4">
    <choose>
      <if variable="title" match="none">
        <text variable="genre"/>
      </if>

        <else-if type="article-newspaper" match="any">
        <text variable="title" font-style="italic"/>
        </else-if>
    </choose>
  </macro>
  <macro name="title-short">
    <choose>
      <if type="book" match="any">
        <text variable="title" form="short" font-style="italic" suffix="..."/>
      </if>
      <else>
        <text variable="title" form="short" quotes="false" font-style="italic" prefix="" suffix="..."/>
      </else>
    </choose>
  </macro>
  <macro name="container-title1">
    <choose>
      <if type="article-journal" match="any">
        <text variable="container-title" prefix="[w:] " font-style="italic"/>
        <choose>
          <if variable="volume">
            <text variable="volume" prefix=" "/>
          </if>
          <else-if variable="issue">
            <text variable="issue" prefix=" "/>
            </else-if>

        </choose>
        <date variable="issued" prefix=", " suffix="">
          <date-part name="year"/>
        </date>
      </if>
      <else-if type="chapter paper-conference" match="any">
        <choose>
          <if variable="container-title">
            <text variable="container-title" quotes="false"  prefix="[w:] " font-style="italic"/>
          </if>
        </choose>
      </else-if>

      <else-if type="entry-encyclopedia" match="any">
        <choose>
          <if variable="container-title">
            <text variable="container-title" prefix="[w:] " font-style="italic"/>
             </if>
        </choose>
      </else-if>
      
      <else-if type="article-magazine" match="any">
        <text variable="container-title" prefix="[w:] " font-style="italic"/>

        <date variable="issued" prefix="" suffix="">
          <date-part name="year"/>
        </date>
             <choose>
          <if variable="volume">
            <text variable="volume" prefix="nr "/>
          </if>
          <else-if variable="issue">
            <text variable="issue" prefix=", nr "/>
            </else-if>

        </choose>
      </else-if>
       <else-if type="article-newspaper" match="any">
        <text variable="container-title" font-style="normal" prefix="[w:] &#x201E;" suffix="&#x201D;"/>
           <if variable="section">
            <text variable="section" prefix=", nr "/>
          </if>

      </else-if>
    </choose>
  </macro>
  <macro name="volume1">
    <choose>
      <if variable="volume">
        <text term="volume" form="short" text-case="lowercase" suffix=" "/>
        <number variable="volume" form="numeric"/>
      </if>
    </choose>
  </macro>
  <!--Pages-->
  <macro name="page">
    <label variable="locator" form="short" suffix=" "/>
    <text variable="locator"/>
  </macro>
  <macro name="page_first">
    <label variable="locator" form="short" suffix=" "/>
    <text variable="locator"/>
  </macro>
  <macro name="page_chapter">
    <choose>
      <if variable="locator">
        <label variable="locator" form="short" suffix=" "/>
        <text variable="locator"/>
      </if>
      <else>
        <label variable="page" form="short" suffix=" "/>
        <text variable="page"/>
      </else>
    </choose>
  </macro>
  <!--other-->
  <macro name="issued1">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="issued-place">
    <text variable="publisher-place" prefix="" suffix=""/>
  </macro>
  <!--Citation-->
  <citation et-al-min="4" et-al-use-first="1" disambiguate-add-names="true" disambiguate-add-givenname="true">
    <layout prefix="" suffix="." delimiter="; ">
      <choose>
        <if position="ibid-with-locator">
          <group delimiter=", ">
            <text term="ibid"/>
            <text macro="page"/>
          </group>
        </if>
        <else-if position="ibid">
          <text term="ibid"/>
        </else-if>
        <else-if position="subsequent">
          <group delimiter=", ">
            <text macro="contributors-short"/>
            <text macro="title-short"/>
            <text macro="page_chapter"/>
          </group>
        </else-if>
        <else>
          <choose>
            <if type="article-journal" match="any">
              <group delimiter=", ">
                <text macro="authors1"/>
                <text macro="title2"/>
                <text macro="container-title1"/>
                <text macro="page_chapter"/>

              </group>
            </if>
            
            <else-if type="article-magazine" match="any">
               <group delimiter=", ">
                <text macro="authors1"/>
                <text macro="title3"/>
                <text macro="container-title1"/>
                <text macro="page_chapter"/>

              </group>
            </else-if>
             <else-if type="article-newspaper" match="any">
               <group delimiter=", ">
                <text macro="authors1"/>
                <text macro="title4"/>
                <text macro="container-title1"/>
                     <group delimiter=" ">
                  <text macro="issued-place"/>
                  <text macro="issued1"/>
                </group>
                <text macro="page_chapter"/>

              </group>
            </else-if>
            
            <else-if type="chapter paper-conference entry-encyclopedia thesis" match="any">
              <group delimiter=", ">
                <text macro="authors1"/>
                <text macro="title2"/>
                <text macro="container-title1"/>
                <text macro="editor-translator"/>
                <text macro="volume1"/>
                <group delimiter=" ">
                  <text macro="issued-place"/>
                  <text macro="issued1"/>
                </group>
              </group>
              <text macro="page_chapter" prefix=", "/>
            </else-if>
            <else-if type="book" match="any">
              <group delimiter=", ">
                <text macro="authors1"/>
                <text macro="title1"/>
                <text macro="volume1"/>
                <group delimiter=" ">
                  <text macro="issued-place"/>
                  <text macro="issued1"/>
                </group>
                <text macro="page_first" prefix=""/>
              </group>
            </else-if>
            <else>
              <group delimiter=", ">
                <text macro="authors1"/>
                <text macro="title1"/>
                <text macro="volume1"/>
                <group delimiter=" ">
                  <text macro="issued-place"/>
                  <text macro="issued1"/>
                </group>
                <text macro="page_first" prefix=""/>
              </group>
            </else>
          </choose>
        </else>
      </choose>
    </layout>
  </citation>
